{
  "name": "create-botmate",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/core/create-botmate",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/core/create-botmate",
        "tsConfig": "packages/core/create-botmate/tsconfig.lib.json",
        "packageJson": "packages/core/create-botmate/package.json",
        "main": "packages/core/create-botmate/src/index.ts",
        "rootDir": "packages/core/create-botmate/src",
        "assets": [
          "packages/core/create-botmate/*.md",
          "packages/core/create-botmate/files/**/*.ejs"
        ]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/core/create-botmate/jest.config.ts"
      }
    }
  }
}
